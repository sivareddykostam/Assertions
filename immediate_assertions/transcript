# waveform.wlf opened as dataset "waveform"
add wave -position insertpoint  \
waveform:/top/clk
add wave -position insertpoint  \
waveform:/top/a
add wave  \
waveform:/top/a
add wave  \
waveform:/top/b
add wave waveform:/top/*
add wave waveform:/top/*
